{
  "required_api_version": "^2.0.0",
  "name": "AWS Resource Search",
  "description": "ULauncher extension to search AWS Resources.",
  "developer_name": "Samuel Victor",
  "icon": "images/aws.png",
  "options": {
    "query_debounce": 0.5
  },
  "preferences": [
    {
      "id": "bucket",
      "type": "keyword",
      "name": "S3 Bucket",
      "description": "Search AWS S3 Buckets.",
      "default_value": "bucket"
    },
    {
      "id": "function",
      "type": "keyword",
      "name": "Lambda Function",
      "description": "Search AWS Lambda Functions.",
      "default_value": "lambda"
    },
    {
      "id": "table",
      "type": "keyword",
      "name": "DynamoDB Table",
      "description": "Search AWS DynamoDB Tables.",
      "default_value": "table"
    },
    {
      "id": "log",
      "type": "keyword",
      "name": "CloudWatch Log Group",
      "description": "Search AWS CloudWatch Log Groups.",
      "default_value": "log"
    },
    {
      "id": "update",
      "type": "keyword",
      "name": "Update AWS Resources",
      "description": "Update AWS Resources.",
      "default_value": "update"
    },
    {
      "id": "profile",
      "type": "input",
      "name": "AWS Profile",
      "description": "AWS Profiles to use. E.g. iam-dev,iam-adm"
    },
    {
      "id": "stages",
      "type": "input",
      "name": "Stages",
      "description": "Which stages to search for. E.g. dev,beta,prod",
      "default_value": "dev,beta,prod"
    },
    {
      "id": "browser",
      "type": "input",
      "name": "Browser",
      "description": "More info on readme.",
      "default_value": "xdg-open"
    },
    {
      "id": "arguments",
      "type": "text",
      "name": "Additional Arguments",
      "description": "Additional arguments to pass to the browser. More info on readme."
    },
    {
      "id": "max_results",
      "type": "input",
      "name": "Max results",
      "description": "Maximum number of results to show.",
      "default_value": "12"
    }
  ]
}
